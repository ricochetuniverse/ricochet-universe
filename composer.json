{
    "name": "",
    "description": "Ricochet Levels",
    "type": "project",
    "license": "MPL-2.0",
    "homepage": "https://ricochet.ngyikp.com",
    "authors": [
        {
            "name": "Ng Yik Phang (ngyikp)",
            "email": "ngyikp@gmail.com",
            "homepage": "https://ngyikp.com"
        }
    ],
    "support": {
        "email": "ngyikp@gmail.com",
        "issues": "https://gitlab.com/ngyikp/ricochet-levels/issues",
        "source": "https://gitlab.com/ngyikp/ricochet-levels",
        "docs": "https://gitlab.com/ngyikp/ricochet-levels"
    },
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "rtconner/laravel-tagging": "~2.2",
        "sentry/sentry-laravel": "^0.10",
        "socialiteproviders/discord": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "deployer/recipes": "^6.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/socialite"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
